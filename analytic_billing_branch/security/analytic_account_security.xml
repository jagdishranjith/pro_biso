<odoo>
    <record id="ir_rule_analytic_account_allowed_billing_branches" model="ir.rule">
        <field name="model_id" ref="analytic.model_account_analytic_account" />
        <field name="domain_force">
            ['|',('billing_branch_ids','=',False),('billing_branch_ids','in',billing_branch_ids)]
        </field>
        <field name="name">Analytic Accounts from allowed billing branches</field>
        <field name="global" eval="True" />
        <field eval="0" name="perm_unlink" />
        <field eval="0" name="perm_write" />
        <field eval="1" name="perm_read" />
        <field eval="0" name="perm_create" />
    </record>
</odoo>
